###  http://www.example.org/nco#day_of_week_in_month

:day_of_week_in_month rdf:type owl:DatatypeProperty ;
                      
                      rdfs:domain :NCODateTimeMoment ;
                      
                      rdfs:range xsd:nonNegativeInteger .



###  http://www.example.org/nco#ncoInStrDateTimeMoment

:ncoInStrDateTimeMoment rdf:type owl:DatatypeProperty ;
                        
                        rdfs:domain :NCOMoment ;
                        
                        rdfs:range xsd:string .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.example.org/nco#NCODateTimeMoment

:NCODateTimeMoment rdf:type owl:Class ;
                   
                   rdfs:subClassOf :NCOTemporalEntity ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:year ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:month ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:week ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:day ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:dayOfWeek ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:dayOfYear ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:hour ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:minute ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:second ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty time:timeZone ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ],
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :day_of_week_in_month ;
                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                   ] .



###  http://www.example.org/nco#NCODateTimeMomentStr

:NCODateTimeMomentStr rdf:type owl:Class ;
                      
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :ncoInStrDateTimeMoment ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger
                                      ] .



###  http://www.example.org/nco#NCOMoment

:NCOMoment rdf:type owl:Class ;
           
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :NCODateTimeMoment
                                               :NCODateTimeMomentStr
                                               time:Instant
                                             )
                               ] .

:NCOListTemporalEntity 
	rdfs:subClassOf rdf:List ;
	rdfs:subClassOf
		[ a owl:Restriction ;
            owl:onProperty rdf:first ;
            owl:allValuesFrom :NCOTemporalEntity 
        ] ;
    rdfs:subClassOf    
        [ a owl:Restriction ;
            owl:onProperty rdf:rest ;
            owl:allValuesFrom :NCOListTemporalEntity 
        ] .

###  http://www.example.org/nco#TemporalRelations

:TemporalRelations rdf:type owl:Class ;
                   
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:oneOf ( time:hasEnd
                                                     :ncoEnd
                                                     :ncoSeqOneOf
                                                     time:hasBeginning
                                                     :ncoBeginning
                                                     :ncoIn
                                                   )
                                       ] .
                                       
time:TemporalEntity rdfs:subClassOf :NCOTemporalEntity .
